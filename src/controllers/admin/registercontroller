const { User } = require("../../models/user");

let registercontroller =(req,res)=>{
     
    console.log(req.body)
    const user =new User(req.body);
    user.save().then((d)=>{
        res.status(200).json({
            msg:"ok"
        });
    }).catch((e)=>{
        res.status(404).json({
            msg:"error"
        });

    });
    
}

module.exports= {registercontroller};